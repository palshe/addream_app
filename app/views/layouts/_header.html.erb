<header class="navbar navbar-fixed-top navbar-inverse">
      <div class="container">
        <%= link_to "Admine", root_path, id:"logo" %>
        <nav>
          <ul id = "navbar-menu"
              class="nav navbar-nav navbar-right collapse navbar-collapse">
            <li><%= link_to "Admineとは？", about_path%></li>
<%comment do%>
            <% if logged_in? %>
              <li><%= link_to "Users", users_path %></li>
              <li class="dropdown">
                <a href="#" id="account" class="dropdown-toggle">
                  Account <b class="caret"></b>
                </a>
                <ul id="dropdown-menu" class="dropdown-menu">
                  <li><%= link_to "Profile", current_user %></li>
                  <li><%= link_to "Settings", edit_user_path(current_user) %></li>
                  <li class="divider"></li>
                  <li>
                    <%= link_to "Log out", logout_path, data:{"turbo-method": :delete}%>
                  </li>
                </ul>
              </li>
            <% else %>
            <%end%>
<%end%>
          </ul>
        </nav>  
      </div>
    </header>